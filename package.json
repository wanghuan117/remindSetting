{
  "name": "ext",
  "version": "0.1.2",
  "description": "ext",
  "scripts": {
    "installCordova": "sh ./install_cordova.sh",
    "runAndroid": "npm run clean && npm run dumpdev && cp -r ./dist/${npm_package_family}/${npm_package_name}/dev/* ./www/ && cordova run android",
    "runIosEmulator": "npm run clean && npm run dumpdev && cp -r ./dist/${npm_package_family}/${npm_package_name}/dev/* ./www/ && cordova run ios",
    "runIosDevice": "npm run clean && npm run dumpdev && cp -r ./dist/${npm_package_family}/${npm_package_name}/dev/* ./www/ && cordova run ios --device",
    "platformAddAndroid": "cordova platform add android",
    "platformAddIOS": "cordova platform add ios",
    "clean": "rm -rf ./dist",
    "pluginAddAll": "cordova plugin add cordova-plugin-whitelist@1.0.0",
    "compile": "rm -rf scripts/dist/* && babel scripts/src --out-dir scripts/dist",
    "start": "npm run compile && dora -p 8001 --plugins \"./scripts/dist/dora/dora-plugin-webpack/lib/index.js,./scripts/dist/dora/dora-plugin-hmr/src/index.js,proxy?port=8005,./scripts/dist/dora/dora-plugin-livereload/src/index.js?enableJs=true&enableCss=true&enableImg=true&enableAll:false\"",
    "build": "npm run compile && rm -rf ./dist/${npm_package_family}/${npm_package_name}/${npm_package_version} && node ./scripts/bin/atool-build -o ./dist/${npm_package_family}/${npm_package_name}/${npm_package_version} -s static",
    "dumpdev": "npm run compile && rm -rf ./dist/${npm_package_family}/${npm_package_name}/dev && node ./scripts/bin/atool-build --no-compress -o ./dist/${npm_package_family}/${npm_package_name}/dev -s static"
  },
  "entry": {
    "index": "./ext/biz/entry/index.jsx"
  },
  "template": "./ext/biz/index.html",
  "theme": {},
  "client": "./demo/",
  "repository": {
    "type": "git",
    "url": "https://git.coding.net/liclu/apollo-mobile-web.git"
  },
  "author": "Apollo Team",
  "license": "MIT",
  "devDependencies": {
    "address": "^1.0.0",
